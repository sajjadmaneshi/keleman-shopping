{
  "name": "keleman-shopping",
  "version": "0.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start optimize": "ng serve  --configuration production  && gzipper c -v ./dist",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "json-server": "json-server --watch db.json",
    "update": "npm i -g npm-check-updates && ncu -u && npm install --force",
    "min": "webpack --config=rtl.config.js",
    "build-compress": "npm version patch & ng build --aot=true && gzipper c -v ./dist",
    "build-compress-watch": "npm version patch & ng build --watch --aot=true && gzipper c -v ./dist",
    "dev:ssr": "ng run keleman-shopping:serve-ssr",
    "serve:ssr": "node dist/keleman-shopping/server/main.js",
    "serve:prerender": "http-server -c-1 dist/keleman-shopping/browser",
    "build:ssr": "ng build --aot=true && gzipper c -v ./dist && ng run keleman-shopping:server",
    "build:ssr-stg": "ng build --aot=true --configuration=stage && gzipper c -v ./dist && ng run keleman-shopping:server",
    "prerender": "ng run keleman-shopping:prerender  && gzipper c -v ./dist",
    "unused-css": "ngx-unused-css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.6",
    "@angular/cdk": "^17.0.3",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/material": "^17.0.3",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@angular/platform-server": "^17.0.6",
    "@angular/router": "^17.0.6",
    "@angular/service-worker": "^17.0.6",
    "@asymmetrik/ngx-leaflet": "^17.0.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/angular-fontawesome": "^0.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@popperjs/core": "^2.11.8",
    "@types/google.maps": "^3.54.10",
    "@types/leaflet": "^1.9.8",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "highcharts": "^11.2.0",
    "highcharts-angular": "^4.0.0",
    "jalali-moment": "^3.3.11",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.0",
    "material-icons": "^1.13.12",
    "ng-otp-input": "^1.9.3",
    "ngx-countdown": "^17.1.0",
    "ngx-dropzone": "^3.1.0",
    "ngx-image-zoom": "^3.0.0",
    "ngx-skeleton-loader": "^8.1.0",
    "rxjs": "~7.8.1",
    "swiper": "^8.4.5",
    "tslib": "^2.6.2",
    "webpack": "^5.89.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular-eslint/schematics": "^17.1.1",
    "@angular/cli": "~17.0.6",
    "@angular/compiler-cli": "^17.0.6",
    "@angular/localize": "^17.0.6",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.4",
    "gzipper": "^7.2.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-unused-css": "^4.0.0-1",
    "typescript": "^5.2.0",
    "webpack-cli": "^5.1.4"
  }
}
